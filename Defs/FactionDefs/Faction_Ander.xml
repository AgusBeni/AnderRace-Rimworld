<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <FactionDef>
        <defName>AnderKingdom</defName>
        <label>Ander Kingdom</label>
        <description>A medieval monarchist society of elf-like people ruled by female supremacy. The
            Ander Kingdom values martial prowess and classical warfare, favoring traditional weapons
            and armor. They maintain a complex relationship with outsiders - sometimes trading,
            sometimes raiding, always proud.</description>
        <pawnSingular>ander</pawnSingular>
        <pawnsPlural>anders</pawnsPlural>
        <categoryTag>Tribal</categoryTag>
        <listOrderPriority>40</listOrderPriority>
        <settlementGenerationWeight>1</settlementGenerationWeight>
        <requiredCountAtGameStart>1</requiredCountAtGameStart>
        <maxCountAtGameStart>3</maxCountAtGameStart>
        <canMakeRandomly>true</canMakeRandomly>
        <canSiege>true</canSiege>
        <canStageAttacks>true</canStageAttacks>
        <leaderTitle>Queen</leaderTitle>
        <factionIconPath>World/WorldObjects/Expanding/Town</factionIconPath>
        <colorSpectrum>
            <li>(0.7, 0.7, 0.9)</li>
            <li>(0.8, 0.8, 0.95)</li>
        </colorSpectrum>
        <factionNameMaker>NamerFactionTribal</factionNameMaker>
        <settlementNameMaker>NamerSettlementTribal</settlementNameMaker>
        <techLevel>Medieval</techLevel>
        <backstoryFilters>
            <li>
                <categories>
                    <li>Tribal</li>
                </categories>
            </li>
        </backstoryFilters>
        <forageabilityFactor>1.0</forageabilityFactor>
        <caravanTraderKinds>
            <li>Caravan_Neolithic_BulkGoods</li>
            <li>Caravan_Neolithic_WarMerchant</li>
        </caravanTraderKinds>
        <visitorTraderKinds>
            <li>Visitor_Neolithic_Standard</li>
        </visitorTraderKinds>
        <baseTraderKinds>
            <li>Base_Neolithic_Standard</li>
        </baseTraderKinds>
        <allowedArrivalTemperatureRange>-40~45</allowedArrivalTemperatureRange>
        <allowedCultures>
            <li>Astropolitan</li>
            <li>Classical</li>
            <li>Medieval</li>
        </allowedCultures>
        <raidCommonalityFromPointsCurve>
            <points>
                <li>(0, 1)</li>
                <li>(3000, 1)</li>
                <li>(4500, 0.5)</li>
                <li>(7000, 0.1)</li>
            </points>
        </raidCommonalityFromPointsCurve>
        <maxPawnCostPerTotalPointsCurve>
            <points>
                <li>(0,35)</li>
                <li>(70,50)</li>
                <li>(700,100)</li>
                <li>(1300,150)</li>
                <li>(100000,10000)</li>
            </points>
        </maxPawnCostPerTotalPointsCurve>
        <pawnGroupMakers>
            <li>
                <kindDef>Combat</kindDef>
                <options>
                    <AnderWarrior>10</AnderWarrior>
                    <AnderArcher>6</AnderArcher>
                    <AnderChief>2</AnderChief>
                </options>
            </li>
            <li>
                <kindDef>Peaceful</kindDef>
                <options>
                    <AnderVillager>10</AnderVillager>
                    <AnderTrader>5</AnderTrader>
                    <AnderChief>2</AnderChief>
                </options>
            </li>
        </pawnGroupMakers>
        <settlementTexturePath>World/WorldObjects/TribalSettlement</settlementTexturePath>
    </FactionDef>
</Defs>