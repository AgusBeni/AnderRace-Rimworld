<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <FactionDef ParentName="FactionBase">
        <defName>AnderKingdom</defName>
        <label>ander kingdom</label>
        <description>A medieval society of elf-like Anders ruled by queens.</description>
        <pawnSingular>ander</pawnSingular>
        <pawnsPlural>anders</pawnsPlural>
        <listOrderPriority>40</listOrderPriority>
        <settlementGenerationWeight>1</settlementGenerationWeight>
        <requiredCountAtGameStart>1</requiredCountAtGameStart>
        <maxCountAtGameStart>3</maxCountAtGameStart>
        <canMakeRandomly>true</canMakeRandomly>
        <canSiege>false</canSiege>
        <canStageAttacks>true</canStageAttacks>
        <leaderTitle>Priest</leaderTitle>
        <factionIconPath>UI/Faction_Ander</factionIconPath>
        <factionNameMaker>NamerFactionTribal</factionNameMaker>
        <settlementNameMaker>AN_NamerTown</settlementNameMaker>
        <colorSpectrum>
            <li>(0.51, 0.58, 0.26)</li>
            <li>(0.89, 1.0, 0.45)</li>
        </colorSpectrum>
        <permanentEnemy>false</permanentEnemy>
        <techLevel>Medieval</techLevel>
        <allowedCultures>
            <li>Corunan</li>
        </allowedCultures>
        <requiredMemes>
            <li MayRequire="Ludeon.RimWorld.Ideology">Loyalist</li>
        </requiredMemes>
        <allowedMemes>

        </allowedMemes>
        <structureMemeWeights>
            <Structure_Ideological MayRequire="Ludeon.RimWorld.Ideology">1</Structure_Ideological>
        </structureMemeWeights>
        <backstoryFilters>
            <li>
                <categories>
                    <li>Tribe</li>
                </categories>
            </li>
        </backstoryFilters>
        <xenotypeSet>
            <xenotypeChances>
                <Ander MayRequire="Ludeon.RimWorld.Biotech">1</Ander>
            </xenotypeChances>
        </xenotypeSet>
        <allowedArrivalTemperatureRange>-40~45</allowedArrivalTemperatureRange>
        <maxPawnCostPerTotalPointsCurve>
            <points>
                <li>(0, 1)</li>
                <li>(3000, 1)</li>
                <li>(4500, 0.5)</li>
                <li>(7000, 0.1)</li>
            </points>
        </maxPawnCostPerTotalPointsCurve>
        <pawnGroupMakers>
            <li>
                <kindDef>Combat</kindDef>
                <commonality>100</commonality>
                <options>
                    <AnderWarrior>10</AnderWarrior>
                    <AnderArcher>10</AnderArcher>
                </options>
            </li>
            <li>
                <kindDef>Peaceful</kindDef>
                <commonality>100</commonality>
                <options>
                    <AnderVillager>10</AnderVillager>
                    <AnderTrader>3</AnderTrader>
                </options>
            </li>
            <li>
                <kindDef>Trader</kindDef>
                <traders>
                    <AnderTrader>1</AnderTrader>
                </traders>
                <guards>
                    <AnderWarrior>25</AnderWarrior>
                    <AnderArcher>25</AnderArcher>
                </guards>
            </li>
        </pawnGroupMakers>
        <settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
        <maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
        <configurationListOrderPriority>30</configurationListOrderPriority>
    </FactionDef>

</Defs>